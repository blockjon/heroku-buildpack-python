# Install dependencies with Pip.
mkdir libspatialindex
cd libspatialindex
curl -O http://download.osgeo.org/libspatialindex/spatialindex-src-1.8.5.tar.gz
tar -zxvf spatialindex-src-1.8.5.tar.gz
cd spatialindex-src-1.8.5
mkdir -p /app/libspatialindex
./configure --prefix=/app/libspatialindex
make
make install

cd ../../
